{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "max-line-length": {
            "options": [
                240
            ]
        },
        "new-parens": false,
        "no-arg": true,
        "no-bitwise": false,
        "no-conditional-assignment": false,
        "no-consecutive-blank-lines": false,
        //"no-any": [true, {"ignore-rest-args": true}],
        "no-console": [
            false
        ],
        "object-literal-sort-keys": false,
        "trailing-comma": [
            false
        ],
        "member-ordering": false,
        "ordered-imports": false,
        "jsdoc-format": true,
        // "completed-docs": [
        //     true,
        //     "methods"
        // ],
        "interface-name": false
    }
}
